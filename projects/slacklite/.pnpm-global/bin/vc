#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/dist/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/dist/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules:/Users/austenallred/clawd/projects/slacklite/.pnpm-global/store/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/dist/vc.js" "$@"
else
  exec node  "$basedir/../store/5/.pnpm/vercel@50.19.1_typescript@5.9.3/node_modules/vercel/dist/vc.js" "$@"
fi
